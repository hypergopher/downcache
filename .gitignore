tls/

# Binaries
bin/
build/
dist/
tmp/

# Env vars
.env*

# UI node modules
node_modules/

# pnpm
.pnpm-debug.log

# Overmind
.overmind.sock

# Ignore pprof output
*.pprof
pprof/*

# Ignore c-tags
tags

# Ignore sqlite test data
/sqlitestore/testdata/
